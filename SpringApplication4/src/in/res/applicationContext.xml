<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    
    <bean class="in.beans.Address" id="addrid">
   		<property name="houseno" value="7653"/>
   		<property name="city" value="Kolkata"/>
   		<property name="pincode" value="987654"/>
   </bean>
   <bean class="in.beans.Student" id="stdid">
   		<property name="rollno" value="231"/>
   		<property name="name" value="Farhan"/>
   		<property name="address" ref="addrid"/>
   </bean>

</beans>